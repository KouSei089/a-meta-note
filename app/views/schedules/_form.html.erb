<%= form_with model: schedule, local: true do |f| %>
  <div>
    <%= f.label "タイトル" %>
    <%= f.date_select :title %>
  </div>
  <div>
    <%= f.label "task_category" %>
  </div>
  <div>
    <%= link_to "category", new_category_path, method: :get,local: true %>
  </div>
  <div>
    <%= f.label "task_title" %>
  </div>
  <div>
    <%= f.label "task_time" %>
  </div>
  <%= f.submit "登録" %>
<% end %>
